{
  "name": "project-report-renderer",
  "description": "Renders project reports to static pages",
  "devDependencies": {
    "@types/node": "^14.6.0",
    "@types/react": "^16.9.46",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.2.5",
    "next": "^9.5.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check"
    }
  },
  "private": true,
  "scripts": {
    "check": "run-s check:format check:lint",
    "check:format": "prettier --write pages",
    "check:lint": "eslint *.ts **/*.ts pages"
  }
}
